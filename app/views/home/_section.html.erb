<section class="gap-2 lg:px-40">
    <%= render partial: "section_header" , locals: {count: count, title: section.title } %>
    <% section.projects.each do |project| %>
        <% cache project do %>
          <%= render partial: "projects/project" , locals: {project: project} %>
        <% end %>
    <% end %>
</section>
